{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "",

   "title": {
    "text": "Government current expenditures: Education",
    "subtitle":"Source: FRED",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "#000000",
    "subtitleColor":"#c55c15"},

  "config": {
    "background":"#ffffff",
    "view": {
      "stroke": "transparent"}},
 
  
  "data": {
    "url": "https://raw.githubusercontent.com/natalytsangarides/natalytsangarides.github.io/main/data_FRED-G160291A027NBEA.json",
    "format": {"property": "observations"}
    },
  

  "height": 300,
  "width": 280,
  

 "mark": {"type": "line", "point": false},

  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"grid": false},
        "title": "Date"
      
      },

    "y": {
      "field": "value",
       "type": "quantitative",
        "title": "value",
        "axis": {"grid": false}
      }
  
  }
  }
