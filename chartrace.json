{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "",

   "title": {
    "text": "Homeownership Rates by Race: Black Alone",
    "subtitle":"Source: FRED",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "#000000",
    "subtitleColor":"#c55c15"},

  "config": {
    "background":"#ffffff",
    "view": {
      "stroke": "transparent"}},
 
  
  "data": {
